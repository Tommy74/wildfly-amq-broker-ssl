/subsystem=messaging-activemq/pooled-connection-factory=activemq-ra:write-attribute(name=user,value="${env.ARTEMIS_USER}")
/subsystem=messaging-activemq/pooled-connection-factory=activemq-ra:write-attribute(name=password,value="${env.ARTEMIS_PASSWORD}")
/subsystem=messaging-activemq/remote-connector=artemis:map-put(name=params, key=ssl-enabled, value=true)
/subsystem=messaging-activemq/remote-connector=artemis:map-put(name=params, key=trustStorePath, value="${env.TRUST_STORE_FILENAME}")
/subsystem=messaging-activemq/remote-connector=artemis:map-put(name=params, key=trustStorePassword, value="${env.TRUSTSTORE_PASSWORD}")
/subsystem=messaging-activemq/remote-connector=artemis:map-put(name=params, key=verify-host, value=false)

